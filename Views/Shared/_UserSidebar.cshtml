@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor
<aside class="main-sidebar sidebar-light-info elevation-4 user-sidebar" style="min-height:100vh;">
    <!-- Brand -->
    <a href="~/" class="brand-link bg-info text-white">
        <img src="~/Admin/dist/img/AdminLTELogo.png" alt="Logo"
             class="brand-image img-circle elevation-3" style="opacity:.9">
        <span class="brand-text font-weight-bold">User Panel</span>
    </a>

    <div class="sidebar">
        <!-- User -->
        <div class="user-panel mt-3 pb-3 mb-3 d-flex">
            <div class="image">
                <img src="~/Admin/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User">
            </div>
            <div class="info">
                <a asp-area="" asp-controller="Account" asp-action="Profile" class="d-block">
                    Hello @HttpContextAccessor.HttpContext.Session.GetString("UserName")
                </a>
            </div>

        </div>

        <!-- Search (optional) -->
        <div class="form-inline">
            <div class="input-group" data-widget="sidebar-search">
                <input class="form-control form-control-sidebar" type="search" placeholder="Search menu…" aria-label="Search">
                <div class="input-group-append">
                    <button class="btn btn-sidebar"><i class="fas fa-search fa-fw"></i></button>
                </div>
            </div>
        </div>

        <!-- Menu -->
        <nav class="mt-2">
            <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">

                <!-- 👨‍⚕️ Search Psychiatrists -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Psychiatrist" asp-action="Search" class="nav-link">
                        <i class="nav-icon fas fa-user-md"></i>
                        <p>Find Psychiatrists</p>
                    </a>
                </li>

             
                <li class="nav-item has-treeview">
                    <a href="#" class="nav-link">
                        <i class="nav-icon fas fa-calendar-plus"></i>
                        <p>
                            Book Appointment
                            <i class="right fas fa-angle-left"></i>
                        </p>
                    </a>
                    <ul class="nav nav-treeview">
                        <li class="nav-item">
                            <a asp-area="User" asp-controller="BookAppointment" asp-action="BookFrontPage" class="nav-link">
                                <i class="far fa-circle nav-icon"></i>
                                <p>New Appointment</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-area="User" asp-controller="BookAppointment" asp-action="Index" class="nav-link">
                                <i class="far fa-circle nav-icon"></i>
                                <p>Booking History</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-area="User" asp-controller="Prescription" asp-action="PrescriptionList" class="nav-link">
                                <i class="far fa-circle nav-icon"></i>
                                <p>Prescription List</p>
                            </a>
                        </li>
                    </ul>
                </li>



                @* <!-- 🗓️ Upcoming & Past Appointment List -->
                <li class="nav-item">
                    <a asp-area="User" asp-controller="BookAppointment" asp-action="Index" class="nav-link">
                        <i class="nav-icon fas fa-calendar-check"></i>
                        <p>My Appointments</p>
                    </a>
                </li> *@

                @* <!-- 🧾 Payments -->
                <li class="nav-item">
                    <a asp-area="User" asp-controller="BookAppointment" asp-action="InvoicePayment" class="nav-link">
                        <i class="nav-icon fas fa-money-check-alt"></i>
                        <p>Payments</p>
                    </a>

                </li> *@

                <!-- 📖 Medical History & Notes -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Records" asp-action="History" class="nav-link">
                        <i class="nav-icon fas fa-file-medical"></i>
                        <p>Medical History</p>
                    </a>
                </li>

                <!-- 💬 Messages -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Messages" asp-action="Index" class="nav-link">
                        <i class="nav-icon fas fa-comments"></i>
                        <p>Messages</p>
                    </a>
                </li>

                <!-- 📥 Notifications -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Notifications" asp-action="Index" class="nav-link">
                        <i class="nav-icon fas fa-bell"></i>
                        <p>Notifications</p>
                    </a>
                </li>

                <!-- 🌡️ Daily Mood Tracker -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Mood" asp-action="Track" class="nav-link">
                        <i class="nav-icon fas fa-thermometer-half"></i>
                        <p>Daily Mood Tracker</p>
                    </a>
                </li>

                <!-- 📌 Progress Chart -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Progress" asp-action="Index" class="nav-link">
                        <i class="nav-icon fas fa-chart-line"></i>
                        <p>Progress Chart</p>
                    </a>
                </li>

                <!-- 🚨 Emergency Consultation -->
                <li class="nav-item">
                    <a asp-area="Users" asp-controller="Emergency" asp-action="Request" class="nav-link text-danger">
                        <i class="nav-icon fas fa-exclamation-triangle"></i>
                        <p>Emergency Consultation</p>
                    </a>
                </li>

                <!-- Logout -->
                <li class="nav-item">
                    <a asp-area="" asp-controller="Home" asp-action="Logout" class="nav-link">
                        <i class="nav-icon fas fa-sign-out-alt"></i>
                        <p>Logout</p>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
</aside>




